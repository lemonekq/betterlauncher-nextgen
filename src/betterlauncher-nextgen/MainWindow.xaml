            </Rectangle.Fill>

            <Rectangle.Triggers>
                <EventTrigger RoutedEvent="Window.Loaded">
                    <BeginStoryboard>
                        <Storyboard>
                            <Storyboard x:Name="versionhandler_blend_bottom_in_opacity">
                                <DoubleAnimation From="0" To="1" Duration="00:00:0.50" BeginTime="00:00:01.35"
                                    Storyboard.TargetName="versionhandler_blend_bottom" 
